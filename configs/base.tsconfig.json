{
    "$schema": "https://schemastore.azurewebsites.net/schemas/json/tsconfig.json",
    "compilerOptions": {
        "skipLibCheck": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "noEmitOnError": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true
    }
}
