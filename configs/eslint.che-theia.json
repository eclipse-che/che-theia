{
    "plugins": [
        "@typescript-eslint",
        "@typescript-eslint/tslint",
        "import",
        "no-null",
        "sonarjs"
    ],
    "rules": {
        "@typescript-eslint/await-thenable": "error",
        "@typescript-eslint/no-unnecessary-type-assertion": "error",
        "sonarjs/no-unused-collection": "error",
        "sonarjs/no-small-switch": "error"
    }
}
