{
    "name": "theia-cypress",
    "version": "0.0.0",
    "description": "E2E tests for Theia",
    "main": "index.js",
    "engines": {
        "node": ">=4.0.0"
    },
    "scripts": {
        "cypress:version": "cypress version",
        "cypress:verify": "cypress verify",
        "cypress:open": "cypress open --global",
        "cypress:run": "node ./scripts/run.js",
        "cypress:run:record": "node ./scripts/run.js --record",
        "cypress:run:chrome": "node ./scripts/run.js --browser chrome",
        "cypress:run:chrome:record": "node ./scripts/run.js --browser chrome --record",
        "test:ci": "run-p --race start cypress:run",
        "test:ci:record": "run-p --race start cypress:run:record",
        "test:ci:chrome": "run-p --race start cypress:run:chrome",
        "test:ci:chrome:record": "run-p --race start cypress:run:chrome:record",
        "start": "node ./scripts/start.js",
        "dev": "nodemon ./scripts/start.js --watch examples",
        "stop-only": "stop-only examples",
        "lint:js": "eslint --fix scripts/*.js"
    },
    "author": "Florent Benoit",
    "license": "EPL-2.0",
    "dependencies": {
        "body-parser": "1.19.0",
        "chai-colors": "1.0.1",
        "chai-date-string": "0.1.0",
        "chai-enzyme": "1.0.0-beta.1",
        "chalk": "4.0.0",
        "csurf": "1.11.0",
        "debug": "4.1.1",
        "dragula": "3.7.2",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "execa": "4.0.0",
        "express": "4.17.1",
        "express-session": "1.17.1",
        "hbs": "4.1.1",
        "http-server": "0.12.3",
        "jquery": "3.5.0",
        "json-server": "0.16.1",
        "lodash": "4.17.15",
        "minimist": "1.2.5",
        "morgan": "1.10.0",
        "react": "16.13.1",
        "react-dom": "16.13.1"
    },
    "devDependencies": {
        "@cypress/snapshot": "2.1.7",
        "@cypress/webpack-preprocessor": "5.1.2",
        "@types/chai": "4.2.11",
        "@types/mocha": "7.0.2",
        "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
        "bluebird": "3.7.2",
        "cypress": "4.5.0",
        "eslint": "6.8.0",
        "eslint-plugin-cypress-dev": "3.0.2",
        "eslint-plugin-mocha": "6.3.0",
        "fs-extra": "9.0.0",
        "glob": "7.1.6",
        "json-server-reset": "1.3.0",
        "nodemon": "2.0.3",
        "npm-run-all": "4.1.5",
        "pretty-ms": "7.0.0",
        "stop-only": "3.1.0",
        "ts-loader": "7.0.2",
        "tsify": "4.0.1",
        "tslint": "6.1.2",
        "typescript": "3.8.3",
        "webpack": "4.43.0"
    }
}
