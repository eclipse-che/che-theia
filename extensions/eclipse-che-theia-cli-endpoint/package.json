{
  "name": "@eclipse-che/theia-cli-endpoint",
  "keywords": [
    "theia-extension"
  ],
  "version": "7.18.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "1.5.0-next.4390a7d6",
    "@eclipse-che/theia-messaging": "7.18.0"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "clean": "rimraf lib",
    "format": "tsfmt -r --useTsfmt ../../configs/tsfmt.json",
    "lint": "eslint --cache=true --no-error-on-unmatched-pattern=true \"{src,test}/**/*.{ts,tsx}\"",
    "compile": "tsc",
    "build": "concurrently -n \"format,lint,compile\" -c \"red,green,blue\" \"yarn format\" \"yarn lint\" \"yarn compile\"",
    "watch": "tsc -w"
  },
  "license": "EPL-2.0",
  "theiaExtensions": [
    {
      "frontend": "lib/browser/cli-frontend-module",
      "backend": "lib/node/cli-backend-module"
    }
  ]
}
